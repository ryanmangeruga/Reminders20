<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">
    <TextView
            android:text="Exam Date"
            android:layout_width="86dp"
            android:layout_height="64dp"
            android:id="@+id/week1r1c1"
            android:textSize="24sp"
            android:layout_alignParentTop="true" android:layout_marginTop="151dp"
            android:textColor="?android:attr/colorAccent" android:textAlignment="center" android:textStyle="bold"
            android:layout_alignParentStart="true" android:layout_marginStart="17dp"/>
    <TextView
            android:layout_width="132dp"
            android:layout_height="64dp"
            android:id="@+id/week1r1c2" android:textSize="24sp" android:textColor="?android:attr/colorAccent"
            tools:text="Morning (8:00 am)" android:layout_alignParentStart="true" android:layout_marginStart="120dp"
            android:layout_alignTop="@+id/week1r1c1" android:textAlignment="center" android:textStyle="bold"/>
    <TextView
            android:text="TextView"
            android:layout_height="64dp" android:layout_alignParentEnd="true"
            android:id="@+id/week1r1c3" tools:text="Afternoon (12:00 pm)"
            android:textSize="24sp" android:textColor="?android:attr/colorAccent"
            android:layout_width="132dp" android:layout_marginEnd="11dp" android:layout_alignTop="@+id/week1r1c1"
            android:textAlignment="center" android:textStyle="bold"/>
    <TextView
            android:text="WEEK 1 SCHEDULE"
            android:layout_width="306dp"
            android:layout_height="57dp"
            android:layout_alignParentTop="true" android:layout_marginTop="43dp" android:id="@+id/header"
            android:layout_centerHorizontal="true" android:textColor="?android:attr/actionMenuTextColor"
            android:textStyle="bold" android:textAlignment="center" android:textSize="32sp"/>
    <TextView
            android:text="TextView"
            android:layout_width="86dp"
            android:layout_height="68dp"
            android:id="@+id/week1r2c1"
            tools:text="Monday 5/6" android:textColor="?android:attr/colorForeground"
            android:textSize="20sp"
            android:textAlignment="center" android:visibility="visible"
            android:layout_alignParentTop="true" android:layout_marginTop="239dp"
            android:layout_alignStart="@+id/week1r1c1" android:textStyle="bold"/>
    <TextView
            android:text="TextView"
            android:layout_width="132dp"
            android:layout_height="68dp"
            android:id="@+id/week1r2c2"
            tools:text="* US Government and Politics" android:textColor="?android:attr/actionMenuTextColor"
            android:layout_alignStart="@+id/week1r1c2" android:layout_alignTop="@+id/week1r2c1"/>
    <TextView
            android:text="TextView"
            android:layout_width="132dp"
            android:layout_height="68dp"
            android:id="@+id/week1r2c3"
            tools:text="* Chinese Language and Culture                    * Environmental Science"
            android:textSize="14sp"
            android:textColor="?android:attr/actionMenuTextColor"
            android:layout_alignTop="@+id/week1r2c1" android:layout_alignStart="@+id/week1r1c3"/>
    <TextView
            android:text="TextView"
            android:layout_width="86dp"
            android:layout_height="68dp" android:layout_alignParentBottom="true"
            android:id="@+id/week1r3c1"
            tools:text="Tuesday 5/7" android:layout_alignEnd="@+id/week1r1c1" android:layout_marginBottom="295dp"
            android:textStyle="bold" android:textColor="?android:attr/actionMenuTextColor" android:textSize="20sp"
            android:textAlignment="center"/>
    <TextView
            android:text="TextView"
            android:layout_width="120dp"
            android:layout_height="68dp"
            android:layout_alignParentBottom="true" android:id="@+id/week1r4c1"
            android:layout_marginBottom="204dp" tools:text="Wednesday 5/8" android:textAlignment="center"
            android:textStyle="bold" android:textSize="20sp" android:textColor="?android:attr/actionMenuTextColor"
            android:layout_alignParentStart="true"/>
    <TextView
            android:text="TextView"
            android:layout_width="86dp"
            android:layout_height="68dp"
            android:layout_alignParentBottom="true" android:id="@+id/week1r5c1" tools:text="Thursday 5/9"
            android:textStyle="bold" android:textColor="?android:attr/actionMenuTextColor" android:textSize="20sp"
            android:layout_marginBottom="115dp" android:textAlignment="center"
            android:layout_alignStart="@+id/week1r1c1"/>
    <TextView
            android:text="TextView"
            android:layout_width="86dp"
            android:layout_height="68dp"
            android:layout_alignParentBottom="true" android:id="@+id/week1r6c1" tools:text="Friday 5/10"
            android:textSize="20sp" android:textColor="?android:attr/actionMenuTextColor" android:textStyle="bold"
            android:textAlignment="center" android:layout_marginBottom="21dp"
            android:layout_alignStart="@+id/week1r1c1"/>
</RelativeLayout>